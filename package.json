{
  "name": "speedscope-widget",
  "version": "1.0.0",
  "description": "Embeddable flamegraph widget based on speedscope",
  "repository": "jlfwong/speedscope",
  "main": "dist/speedscopeWidget.js",
  "module": "dist/speedscopeWidget.js",
  "bin": {
    "speedscope": "./bin/cli.mjs"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "rm -rf dist && esbuild src/speedscopeWidget.ts --outdir=dist --format=esm --platform=browser --bundle --splitting --chunk-names=libs/[name]-[hash] --loader:.wasm=binary",
    "test": "rm -rf dist && esbuild src/widgetApiLocalTestMain.ts --outdir=dist --format=esm --platform=browser --bundle --splitting --chunk-names=libs/[name]-[hash] --loader:.wasm=binary",
    "local": "npx serve -l 64906 && npm run test",
    "update-standalone": "rm -rf dist && esbuild src/widgetUtils.tsx --outdir=dist --format=esm --platform=browser --bundle --loader:.wasm=binary && node scripts/standaloneTest.js",
    "build-standalone": "rm -rf dist/standalone && esbuild src/widget.tsx --outdir=dist/standalone --format=esm --platform=browser --bundle --loader:.wasm=binary && node scripts/standalone.js",
    "build-demo": "rm -rf dist && esbuild src/widgetUtils.tsx --outdir=dist --format=esm --platform=browser --bundle --loader:.wasm=binary && node scripts/build-demo.js",
    "prepublishOnly": "npm run build",
    "publish-widget": "node scripts/publish-widget.mjs"
  },
  "files": [
    "dist/speedscopeWidget.js",
    "dist/speedscopeWidget.css",
    "dist/libs/**",
    "bin/cli.mjs",
    "README-WIDGET.md",
    "!*.map"
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "author": "jlfwong",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@types/jszip": "3.1.4",
    "@types/node": "14.0.1",
    "@types/pako": "1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "acorn": "7.2.0",
    "aphrodite": "2.1.0",
    "esbuild": "^0.25.8",
    "eslint": "^8.57.1",
    "jest": "24.3.0",
    "jsverify": "0.8.3",
    "jszip": "3.1.5",
    "pako": "1.0.6",
    "preact": "10.4.1",
    "prettier": "3.1.1",
    "protobufjs": "6.8.8",
    "source-map": "0.6.1",
    "ts-jest": "24.3.0",
    "tsx": "4.19.2",
    "typescript": "5.3.3",
    "typescript-json-schema": "0.42.0",
    "uglify-es": "3.2.2",
    "uint8array-json-parser": "0.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./src/jest-setup.js"
    ],
    "testRegex": "\\.test\\.tsx?$",
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "open": "10.1.0"
  }
}
